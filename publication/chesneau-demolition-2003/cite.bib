 month = {October},
 note = {Number: 4
Publisher: Association des amis de la Revue de Géographie de Lyon},
 number = {Vol. 78/4},
 pages = {337--348},
 shorttitle = {La démolition des bureaux en Île-de-France},
 title = {La démolition des bureaux en Île-de-France : renouvellement ou flexibilité ?},
