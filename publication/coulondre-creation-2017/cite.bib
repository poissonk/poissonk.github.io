@article{coulondre_creation_2017,
 abstract = {Cet article interroge les modes de création de profit par les acteurs économiques à travers le cas des promoteurs immobiliers. En reconstituant l’histoire d’un grand projet de centre commercial en France et en confrontant cet historique aux documents comptables prévisionnels rédigés par le promoteur, nous montrons que, contrairement à ce que prévoient les approches marxistes centrées sur l’exploitation des valeurs foncières, le profit s’inscrit avant tout dans un travail entrepreneurial de qualification du bien immobilier. Le projet est un système d’acteurs que le promoteur tente de coordonner en intégrant les attentes de ses interlocuteurs stratégiques (élus locaux, locataires, investisseurs) dans les propriétés des édifices qu’il conçoit. Ce faisant, il obtient l’implication de ces protagonistes sans s’appuyer uniquement sur une coordination par les prix qui conduirait à terme à réduire la marge bénéficiaire collectée lors du projet.},
 author = {Coulondre, Alexandre},
