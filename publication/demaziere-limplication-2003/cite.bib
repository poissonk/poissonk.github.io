Publisher: Association des amis de la Revue de Géographie de Lyon},
 number = {Vol. 78/4},
 pages = {363--373},
 shorttitle = {L’implication des pouvoirs publics locaux dans la production d’immobilier d’entreprise},
 title = {L’implication des pouvoirs publics locaux dans la production d’immobilier d’entreprise : le cas des centres d’appels dans deux agglomérations de taille moyenne, Orléans et Tours},
