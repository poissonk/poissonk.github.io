 institution = {Centre d'études sur les réseaux, les transports, l'urbanisme et les constructions publiques (CERTU)},
 keywords = {Bâti, Cadastre, Données fiscales, Foncier, Information géographique, Parcelle, Partenariats, SIG, Statistiques},
 month = {June},
