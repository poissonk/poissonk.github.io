@article{cuyala_diffusion_2014,
 abstract = {La recherche en géographie théorique et quantitative est caractérisée par une forte dissymétrie. Très ancrée dans le Sud-Est de la France, elle est minoritaire ou inexistante dans les centres universitaires du Sud-Ouest. De quel processus de diffusion spatiale cette configuration est-elle le résultat ? Cet article propose une entrée particulière pour étudier la diffusion d’un mouvement scientifique : l’affiliation des acteurs au mouvement, à partir du Répertoire des géographes français, source d’information systématique ayant été rééditée tous les quatre ans de 1969 à 2007. La mise en place d’une méthode d’analyse des distributions spatio-temporelles des thèmes de recherche montre que cette répartition est le résultat d’un processus de diffusion hiérarchique qui forme un réseau de lieux d’amorce puis de diffusion consolidée.},
 author = {Cuyala, Sylvain and Commenges, Hadrien},
 file = {Full Text PDF:C\:\\Users\\poiss\\Zotero\\storage\\2KC4G6K4\\Cuyala et Commenges - 2014 - La diffusion de la géographie théorique et quanti.pdf:application/pdf;Snapshot:C\:\\Users\\poiss\\Zotero\\storage\\IP8ZN4AB\\halshs-01163927.html:text/html},
 journal = {Espace Geographique},
 language = {fr},
 number = {4},
 pages = {289},
 shorttitle = {La diffusion de la "géographie théorique et quantitative" en France},
 title = {La diffusion de la "géographie théorique et quantitative" en France : quel modèle de diffusion spatiale ?},
 url = {https://halshs.archives-ouvertes.fr/halshs-01163927},
 urldate = {2021-09-01},
 volume = {43},
 year = {2014}
}

