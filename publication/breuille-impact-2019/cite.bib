@article{breuille_impact_2019,
 abstract = {Cet article analyse l’impact de la densité, mesurée alternativement par l’emprise au sol du bâti et la tache urbaine, et des formes urbaines (surface urbanisée, indice de Gini et distance moyenne entre deux habitants) sur les dépenses de fonctionnement et d’investissement à l’échelle des communes pour dix aires urbaines françaises en 2015. Nous montrons que la densification entraîne dans l’ensemble une baisse des dépenses publiques mais que ses effets sont hétérogènes et complexes selon la forme urbaine. Les bénéfices de la densification pour les dépenses de fonctionnement, et également de façon moins prononcée pour les dépenses d’investissement, concernent surtout les zones centrales et les communes de faible densité dotées d’un centre-bourg.Classification JEL : C26, H72, R12, R51.},
 author = {Breuillé, Marie-Laure and Grivault, Camille and Gallo, Julie Le and Goix, Renaud Le},
