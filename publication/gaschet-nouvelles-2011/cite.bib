@article{gaschet_nouvelles_2011,
 abstract = {This article aims at identifying emergent secondary centers within the Bordeaux metropolitan area and measuring their impact on residential and commercial real estate values, through the estimate of hedonic gradients. The various scales of the urban polycentrism are explored through the taking into account of two types of subcenters (by emergence and integration of satellite towns). Two main trends of urban centrality are explored. The residential centrality is recomposed by the increasing influence of urban amenities at the expect of employment accessibility. Economic centrality is increasingly differentiated from residential centrality.},
 author = {Gaschet, Frédéric and Pouyanne, Guillaume},
